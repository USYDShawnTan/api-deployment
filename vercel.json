{
  "version": 2,
  "buildCommand": "",
  "outputDirectory": ".",
  "installCommand": "",
  "regions": ["sin1"],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/run.py"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ],
  "functions": {
    "api/run.py": {
      "runtime": "python3.10",
      "memory": 1024
    }
  },
  "env": {
    "PYTHON_VERSION": "3.10",
    "MONGODB_URI": "@mongodb_uri",
    "MONGODB_DB_NAME": "@mongodb_db_name",
    "API_ACCESS_PASSWORD": "@api_access_password"
  }
}
